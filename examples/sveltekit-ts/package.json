{
	"name": "sveltekit-ts",
	"version": "0.2.1",
	"scripts": {
		"sw-dev": "vite dev",
		"dev": "vite dev",
		"build-generate-sw": "GENERATE_SW=true vite build",
		"build-generate-sw-node": "NODE_ADAPTER=true GENERATE_SW=true vite build",
		"build-inject-manifest": "vite build",
		"build-inject-manifest-node": "NODE_ADAPTER=true vite build",
		"build-self-destroying": "SELF_DESTROYING_SW=true vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"lint-fix": "nr lint --fix",
		"test-generate-sw": "nr build-generate-sw && GENERATE_SW=true vitest run",
		"test-generate-sw-node": "nr build-generate-sw-node && NODE_ADAPTER=true GENERATE_SW=true vitest run",
		"test-inject-manifest": "nr build-inject-manifest && vitest run",
		"test-inject-manifest-node": "nr build-inject-manifest-node && NODE_ADAPTER=true vitest run",
		"test": "nr test-generate-sw && nr test-generate-sw-node && nr test-inject-manifest && nr test-inject-manifest-node"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "latest",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"@vite-pwa/sveltekit": "workspace:*",
		"eslint": "^8.40.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"svelte": "^3.59.0",
		"svelte-check": "^3.3.1",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5",
		"vitest": "^0.31.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.10"
	}
}
